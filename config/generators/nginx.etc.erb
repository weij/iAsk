user	<%= user %>; 
worker_processes	6; 
error_log	<%= shared_path %>/log/nginx_error.log;
pid	<%= nginx_pid %>;

events {
	worker_connections  1024;
	# multi_accept on;
}

http {
	include		mime.types;
	access_log	<%= shared_path %>/log/nginx_access.log;
	sendfile	on;
	#tcp_nopush	on; 
	
	#keepalive_timeout  0; 
	keepalive_timeout  65;
	tcp_nodelay        on;
	
	gzip  on;
	gzip_disable "MSIE [1-6]\.(?!.*SV1)";
	
	include	conf.d/*.conf;
}